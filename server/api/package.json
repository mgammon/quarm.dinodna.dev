    "@types/express": "^4.17.17",
    "@types/gunzip-maybe": "^1.4.2",
    "@types/jest": "^29.5.2",
    "@types/moment": "^2.13.0",
    "@types/node": "^20.3.1",
    "@types/supertest": "^2.0.12",
    "@types/tar-fs": "^2.0.4",
    "@types/tar-stream": "^3.1.3",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
